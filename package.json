    "auth0-lock": "^11.26.3",
    "auth0-lock-passwordless": "^2.2.3",
    "auth0-react-lock": "^1.0.4",
    "babel-plugin-styled-components": "^1.10.7",
    "cosmicjs": "^3.3.1",
    "emailjs-com": "^2.6.3",
    "gatsby": "^2.23.17",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-google-analytics": "^2.3.9",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-modal-routing": "^1.2.0",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
